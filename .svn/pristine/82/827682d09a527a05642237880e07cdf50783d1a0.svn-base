    float: left;
    width: 26px;
    height: 26px;
    margin-bottom: 10px;
    text-indent: -9999px;
    cursor: pointer;
    -webkit-box-shadow: inset 0 0 0 1px rgba(0, 0, 0, 0.2);
    -moz-box-shadow: inset 0 0 0 1px rgba(0, 0, 0, 0.2);
    box-shadow: inset 0 0 0 1px rgba(0, 0, 0, 0.2);
}
#style_switcher ul li + li {
    margin-left: 10px;
}
#style_switcher ul li:nth-child(5n+6) {
    clear: left;
    margin-left: 0;
}
#style_switcher .style_items + .style_items {
    margin-top: 20px;
}
#style_switcher .style_title {
    font-size: 12px;
    margin-bottom: 12px;
    padding-bottom: 2px;
    border-bottom: 1px dashed #e1e1e1;
}
#style_switcher .radio {
    font-size: 12px;
    padding-top: 0;
}
#style_switcher .radio input {
    margin-top: 2px;
}
#style_switcher .style_active {
    -webkit-border-radius: 26px;
    -moz-border-radius: 26px;
    border-radius: 26px;
}
#style_switcher #style_reset {
    margin-top: 20px;
}
/* media queries */
.container-fluid {
    background: none repeat scroll 0 0 #0078d7 !important;
    margin-right: auto;
    padding-left: 20px;
    padding-right: 20px;
    position: relative;
    z-index: 999999;
}
@media (max-width: 767px) {
    [class*='col-sm'] {
        margin-bottom: 20px;
    }
    .col-sm-12 {
        margin-bottom: 0;
    }
    .page_bar [class*='col-sm'].text-right {
        text-align: left;
    }
    .container-fluid {
        padding-right: 20px;
        padding-left: 10px;
        background: #368ee0 !important;
    }
}
@media (max-width: 991px) {
    body {
         padding-top: 0px;
    }
    #main_wrapper {
        /* margin-left: 50px;*/
    }
    [class*='col-md'] {
        margin-bottom: 20px;
    }
    .col-md-12 {
        margin-bottom: 0;
    }
    label[class*='col-md'] + [class*='col-md'] {
        margin-top: 0;
    }
    .page_bar [class*='col-md'].text-right {
        text-align: left;
    }
}
@media (max-width: 1200px) {
    [class*='col-lg'] {
        margin-bottom: 20px;
    }
    .col-lg-12 {
        margin-bottom: 0;
    }
    label[class*='col-lg'] + [class*='col-lg'] {
        margin-top: 0;
    }
    .page_bar [class*='col-lg'].text-right {
        text-align: left;
    }
}
