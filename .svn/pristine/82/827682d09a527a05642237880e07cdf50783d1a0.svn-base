/* Checkbox and Radio*/
input[type="checkbox"], input[type="radio"] {
    position: relative;
    height: 14px;
    width: 14px;
}
input[type="checkbox"]:after,
input[type="radio"]:after {
    content: '';
    display: block;
    background-color: #f3f3f3;
    pointer-events: none;
    font-size: 15px;
    position: absolute;
    top: 0px;
    left: 0px;
    height: inherit;
    width: inherit;
    color: transparent;
    border-radius: 4px;
    line-height: 15px;
    background-position: .25rem 4.25rem;
    background-repeat: no-repeat;
    background-size: 67%;
    border: #adb5bd solid 1px;
    -webkit-transition: background-color .15s ease-in-out,border-color .15s ease-in-out,-webkit-box-shadow .15s ease-in-out;
    transition: background-color .15s ease-in-out,border-color .15s ease-in-out,-webkit-box-shadow .15s ease-in-out;
    -o-transition: background-color .15s ease-in-out,border-color .15s ease-in-out,box-shadow .15s ease-in-out;
    transition: background-color .15s ease-in-out,border-color .15s ease-in-out,box-shadow .15s ease-in-out;
    transition: background-color .15s ease-in-out,border-color .15s ease-in-out,box-shadow .15s ease-in-out,-webkit-box-shadow .15s ease-in-out;
}
input[type="checkbox"]:after {
    background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 8 8'%3e%3cpath fill='%23fff' d='M6.564.75l-3.59 3.612-1.538-1.55L0 4.26 2.974 7.25 8 2.193z'/%3e%3c/svg%3e");
}
input[type="radio"]:after {
    background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='-4 -4 8 8'%3e%3ccircle r='3' fill='%23fff'/%3e%3c/svg%3e");
    border-radius: 50%;
}
input[type="checkbox"]:checked:after,
input[type="radio"]:checked:after {
    background-color: #007bff;
    border-color: #007bff;
    background-position: center;
}
input[type="checkbox"]:focus:after,
input[type="radio"]:focus:after {
    -webkit-box-shadow: 0 0 0 0.25rem rgba(0,123,255,.25);
    box-shadow: 0 0 0 0.25rem rgba(0,123,255,.25);
}
input[type="checkbox"]:not(:checked):disabled:after,
input[type="radio"]:not(:checked):disabled:after {
    background: #e9ecef;
}
input[type="checkbox"]:checked:disabled:after,
input[type="radio"]:checked:disabled:after {
    background-color: #d1d1d1;
    border-color: #bdbdbd;
}
/* Checkbox and Radio reset*/
.default-ui input[type="checkbox"]:after,
.default-ui input[type="radio"]:after,
input[type="checkbox"].default-ui:after,
input[type="radio"].default-ui:after {
    content: none;
}
:not(.custom-cb) > .multiselect-container input[type="checkbox"]:after,
:not(.custom-cb) > .multiselect-container input[type="radio"]:after {
    content: none;
}

