.container-fluid{ background-color:#1E3F20 !important; }
#navigation .main-nav > li > a:hover {   text-decoration: none;    background: #345830 !important;}
#navigation .main-nav > li.open > a {    background: #345830 !important;    text-decoration: none;}
#navigation .dropdown-menu {/*    border-color: #345830 !important;*/    border-top-style: none;    border-top-width: 0;}
#navigation .dropdown-menu > li > a:hover {    background: #345830;    color: #fff;    filter: none;}
#navigation .dropdown-menu > li.dropdown-submenu .dropdown-menu {    border-top: 2px solid #345830;    -moz-border-radius: 0;    -webkit-border-radius: 0;    border-radius: 0;    margin-left: 0;}
#navigation .dropdown-menu > li.dropdown-submenu:hover > a, #navigation .dropdown-menu > li.dropdown-submenu:focus > a {    background: #345830;    color: #fff;}
.multi-column-dropdown li a:hover {    text-decoration: none;    color: #fff;    background-color: #345830 ;}
#navigation .user .dropdown.open {    background: #345830 !important;}
#navigation .user .icon-nav > li > a:hover {    background: #345830 !important;    text-decoration: none;}
.name_scroll:hover {    background:#345830 !important;    color: #FFFFFF !important;    filter: none !important;    text-decoration:none;}


